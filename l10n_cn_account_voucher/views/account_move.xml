<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--print account move -->
        <report
            id="action_report_account_move"
            string="Voucher"
            model="account.move"
            report_type="qweb-pdf"
            file="l10n_cn_account_voucher.report_account_move"
            name="l10n_cn_account_voucher.report_account_move"
        />


        <record id="view_account_move_form_inherit_print" model="ir.ui.view">
           <field name="name">account.move.form.inherit.print</field>
           <field name="model">account.move</field>
           <field name="inherit_id" ref="account.view_move_form"/>
           <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="chinese_sequence_number"/>
                </field>
           </field>
        </record>

        <record id="view_move_form_inherit" model="ir.ui.view">
            <field name="name">account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="proof"/>
                </field>
            </field>
        </record>
    </data>
</openerp>
